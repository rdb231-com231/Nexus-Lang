def unpack using(lst)
    check len(lst) >= 3 as "List must have 3 elements max. You can run: [[1, 2], [3, 4], [5, 6]]"

    return get(lst, 0), get(lst, 1), get(lst, 2)
end

def map using(lst, func):
    check islist(lst)
    check isfunc(func)

    range i = 0, to len(lst) do
        func(get(lst, i))
    end